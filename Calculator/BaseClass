public class baseclass {
	public static void main(String[] args) {
		area ourCalculator = new area();
		double addresult = ourCalculator.sum(10,20);
		double difresult = ourCalculator.dif(100, 50);
		double mulresult = ourCalculator.mul(15, 25);
		double divresult = ourCalculator.div(100, 50);
		double sqrresult = ourCalculator.sqr(100);
		double sqrtresult = ourCalculator.sqrt(200.0);
		double modresult = ourCalculator.mod(100, 22);
		double squareresult = ourCalculator.square(8);
		double rectangleresult = ourCalculator.rectangle(20,40);
		float circleresult = ourCalculator.circle(50);
		System.out.println(addresult);
		System.out.println(difresult);
		System.out.println(mulresult);
		System.out.println(divresult);
		System.out.println(sqrresult);
		System.out.println(sqrtresult);
		System.out.println(modresult);
		System.out.println(squareresult);
		System.out.println(rectangleresult);
		System.out.println(circleresult);
		
	}
}
